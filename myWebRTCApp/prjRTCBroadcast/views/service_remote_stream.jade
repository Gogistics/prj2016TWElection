div(class="remoteStream" ng-controller="RemoteStreamsController as rtc")
  div(class="col-xs-12")
    h3 Remote Users List
    input(type="button" class="btn btn-default" value="Refresh" ng-click="rtc.loadData()" )
    br
    table
      thead
        tr
          th Coming Soon...
      tbody
        tr(ng-repeat="stream in rtc.remoteStreams" ng-class="{ 'selected' : stream.isPlaying }")
          td
            span {{stream.name}}
